{
    "name": "user_management",
    "role": "user_management",
    "tables": {
            "cms_users": [
            "id int unsigned not null auto_increment unique primary key",
            "user_first_name varchar(100)",
            "user_middle_name varchar(200)",
            "user_last_name varchar(100)",
            "access_group int unsigned not null",
            "date_created timestamp default current_timestamp",
            "date_changed timestamp default current_timestamp on update current_timestamp"
        ],
        "cms_user_auth": [
            "id int unsigned not null unique auto_increment primary key",
            "user_name varchar(200) not null unique",
            "password varbinary({pass_size}) not null unique",
            "salt varbinary({salt_size}) not null unique"
        ],
        "session": [
            "session_id"
        ]
    }
}